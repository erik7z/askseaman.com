  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "typesgen": "graphql-codegen --config typesgen.yml",
    "docker:build": "source ./../.env && docker build -t askseaman-client-build --build-arg REACT_APP_GRAPHQL_HOST=$REACT_APP_GRAPHQL_HOST --build-arg REACT_APP_GRAPHQL_PORT=$REACT_APP_GRAPHQL_PORT . && docker tag askseaman-client-build zzzloy777/askseaman-client-build:latest && docker push zzzloy777/askseaman-client-build:latest",
    "docker:build:local": "source ./../.env.local && docker build -t askseaman-client-build-local --build-arg REACT_APP_GRAPHQL_HOST=$REACT_APP_GRAPHQL_HOST --build-arg REACT_APP_GRAPHQL_PORT=$REACT_APP_GRAPHQL_PORT . && docker tag askseaman-client-build-local zzzloy777/askseaman-client-build-local:latest && docker push zzzloy777/askseaman-client-build-local:latest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.1",
    "@graphql-codegen/typescript": "1.20.2",
    "@graphql-codegen/typescript-operations": "1.17.14",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@types/apollo-upload-client": "14.1.0",
    "@types/he": "1.1.1",
    "@types/react-tag-autocomplete": "6.1.0"
  }
